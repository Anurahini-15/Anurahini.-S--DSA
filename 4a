int main() {
    Stack s;
    initialize(&s);

    // Demonstrating the usage of the stack
    push(&s, 10);
    push(&s, 20);
    push(&s, 30);
    push(&s, 40);

    printf("Top element: %d\n", peek(&s));

    printf("Popped element: %d\n", pop(&s));

    printf("Top element after pop: %d\n", peek(&s));

    return 0;
}

